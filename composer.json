{
    "name": "franzip/serp-scraper",
    "type": "library",
    "description": "A library to extract, serialize and store data scraped on Search Engine result pages.",
    "license": "MIT",
    "homepage": "http://github.com/franzip/serp-scraper",
    "keywords": ["scrape", "scraper", "serp", "data", "harvesting", "search-engine", "google"],
    "authors": [{
        "name": "Francesco Pezzella",
        "email": "franzpezzella@gmail.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "franzip/throttler": "0.2.*@dev",
        "franzip/serp-fetcher": "0.2.*@dev",
        "franzip/serp-page-serializer": "0.2.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "seld/jsonlint": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Franzip\\SerpScraper\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
